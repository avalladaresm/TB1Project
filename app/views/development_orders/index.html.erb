<h1>Development Orders</h1>

<% if DevelopmentOrder.any?%>
    <h2>Listado</h2>
    <table border="1">
    
        <tr>
            <th>ID</th>
            <th>Nombre Proyecto</th>
            <th>Tecnologia a usar</th>
        </tr>

        <% @development_orders.each do |development_order| %>
            <tr>
                <td><%= link_to development_order.id, development_order %></td>
                <td><%= development_order.PROJECTNAME_DEVORDER %></td>
                <td><%= development_order.TECHS_DEVORDER %></td>
            <tr>
        <% end %>
    </table>
<% else %>
<em>No hay ninguna orden de desarrollo en la tabla</em>
<% end %>

<p>
<%= link_to "Nueva Order de Desarrollo", new_development_order_path %>
</p>
